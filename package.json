{
  "name": "wp-vimeo-embed",
  "version": "0.3.0",
  "description": "A collection of Vimeo embed tools and shortcodes for use in WordPress themes.",
  "keywords": [
    "ideasonpurpose",
    "video",
    "vimeo",
    "wordpress"
  ],
  "homepage": "https://github.com/ideasonpurpose/wp-vimeo-embed#readme",
  "bugs": {
    "url": "https://github.com/ideasonpurpose/wp-vimeo-embed/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ideasonpurpose/wp-vimeo-embed.git"
  },
  "license": "MIT",
  "author": "Joe Maller <joe@joemaller.com> (http://www.joemaller.com)",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test_old": "./vendor/bin/phpunit --colors=always tests",
    "test": "docker-compose run test",
    "version": "version-everything && git add -u",
    "watch_old": "watch -f '**/*.php' -r './vendor/bin/phpunit --colors=always tests'",
    "watch": "watch -f '**/*.php' -r 'npm run test'",
    "prettier": "prettier"
  },
  "devDependencies": {
    "@prettier/plugin-php": "^0.10.2",
    "cli-watch": "^1.0.7",
    "prettier": "^1.17.0",
    "version-everything": "^0.2.2"
  },
  "version_files": [
    "README.md",
    "src/VimeoEmbed.php"
  ]
}
