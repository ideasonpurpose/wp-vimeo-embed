{
  "name": "ideasonpurpose/wp-vimeo-embed",
  "description": "Vimeo embedding functions and shortcodes",
  "license": "ISC",
  "keywords": [
    "wordpress",
    "vimeo",
    "video",
    "Ideas On Purpose",
    "ideasonpurpose"
  ],
  "author": "Ideas On Purpose (https://www.ideasonpurpose.com/)",
  "contributors": [
    "Joe Maller <joe@ideasonpurpose.com>"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "ideasonpurpose\\": "src"
    }
  },
  "require-dev": {
    "brain/monkey": "^2.2",
    "php-coveralls/php-coveralls": "^2.1",
    "phpunit/phpunit": "^7"
  },
  "require": {
    "guzzlehttp/guzzle": "^6.3",
    "wikimedia/relpath": "^2.1"
  }
}
